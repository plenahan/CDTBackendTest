<%- include('../define') %>
<h2 class="page-header">Search Rocks</h2>
<form action="/rocks" method="GET">
    <label>Title</label>
    <input type="text" name="title" value="<%= searchOptions.title %>">
    <button class="btn btn-primary" type="submit">Search</button>
</form>
<br>
<% rock.forEach(rock => { %>
    <div><%= rock.title %></div>
    <div class="btn-row">
        <a class="btn btn-primary" href="/rocks/<%= rock.id %>">View</a>
        <a class="btn btn-primary" href="/rocks/<%= rock.id %>/edit">Edit</a>
        <%- include('../../partials/deleteForm', { url: `/rocks/${rock.id}` }) %>
    </div>
<% }) %>